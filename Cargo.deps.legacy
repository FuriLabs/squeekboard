# Dependencies which change based on build flags
bitflags = "1.0.*"
clap = { version = "2.32.*", default-features = false }
regex = { version = "1.1.*", default-features = false, features = ['use_std'] }

[dependencies.cairo-rs]
version = "0.5.*"

[dependencies.gdk]
version = "0.9.*"

[dependencies.gio]
version = "0.5.*"
features = ["v2_44"]

[dependencies.glib]
version = "0.6.*"
features = ["v2_44"]

[dependencies.gtk]
version = "0.5.*"
features = ["v3_22"]
